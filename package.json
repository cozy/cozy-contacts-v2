{
  "name": "cozy-contacts",
  "version": "0.0.1",
  "description": "A cozy application to manage your contacts.",
  "main": "server.coffee",
  "dependencies": {
    "jugglingdb": "~0.2.0-25",
    "jugglingdb-cozy-adapter": "~0.3.5",
    "cozy-i18n-helper": "~0.0.1",
    "express": "~3.1.0",
    "async": "~0.2.9",
    "jade": "~0.32.0"
  },
  "devDependencies": {
    "chai": "~1.5.0",
    "request-json": "=0.3.0",
    "mocha": "~1.9.0",
    "sinon": "~1.6.0",
    "coffee-script": "*"
  },
  "scripts": {
    "test": "mocha server/_specs/ --reporter spec --compilers coffee:coffee-script --colors",
    "start": "coffee server.coffee"
  },
  "repository": "https://github.com/mycozycloud/cozy-contacts",
  "author": "Romain M. Foucault",
  "readmeFilename": "README.md",
  "cozy-permissions": {
    "Contact": {
      "description": "Creates and edits your contacts."
    },
    "CozyInstance": {
      "description": "Read language setting"
    }
  },
  "cozy-widget": "/apps/contacts/widget"
}
